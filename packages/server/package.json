{
  "name": "@graham/server",
  "packageManager": "yarn@4.1.1",
  "private": true,
  "scripts": {
    "build": "node build.js &&  [ -f .env ]  && cp .env dist/.env || exit 0",
    "start": "node dist/index.js",
    "dev": "tsx --watch index.ts",
    "test": "vitest --no-isolate --maxConcurrency=20",
    "type:check": "tsc --noEmit",
    "changeset": "changeset",
    "ci:publish": "changeset publish",
    "ci:version": "changeset version && python3 .github/update_versions.py"
  },
  "main": "index.js",
  "devDependencies": {
    "@graham/db": "workspace:^",
    "@graham/shared": "workspace:^",
    "@trigger.dev/build": "3.3.12",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "esbuild": "^0.21.4",
    "esbuild-plugin-tsc": "^0.4.0",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.10.5",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@axiomhq/winston": "^1.3.0",
    "@browserbasehq/sdk": "^2.0.0",
    "@browserbasehq/stagehand": "^1.7.0",
    "@fastify/cors": "^10.0.1",
    "@slack/web-api": "^7.8.0",
    "@trigger.dev/sdk": "3.3.12",
    "@upstash/qstash": "^2.7.20",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fastify": "^5.2.0",
    "openai": "^4.76.3",
    "winston": "^3.17.0",
    "zod": "^3.24.1"
  }
}
